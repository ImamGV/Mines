local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()
local Window = Flux:Window("YOUHUB", "Mines", Color3.fromRGB(0,255,225), Enum.KeyCode.LeftControl)
local Tab = Window:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")
Tab:Toggle("Auto Collects Items", "Automatically collects items for you!", function(v)
_G.AutoCollect = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoCollect then
for _,v in ipairs(workspace.Items:GetDescendants()) do
if v:IsA("BasePart") then
v.CFrame = game.Players.LocalPlayer.Character.Head.CFrame
end
end
end
end)
end)
Tab:Button("Bring All (Must have)", "Collects items button!", function()
for _,v in ipairs(workspace.Items:GetDescendants()) do
if v:IsA("BasePart") then
v.CFrame = game.Players.LocalPlayer.Character.Head.CFrame
end
end
end)
Tab:Line()
Tab:Toggle("Auto Sell Inventory (Near)", "Automatically sell mines items for you!", function(v)
_G.AutoSell = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoSell then
local Event = game:GetService("ReplicatedStorage").Ml.SellInventory
Event:FireServer()
end
end)
end)
Tab:Button("Sell Inventory (Near)", "Sell mines items button!", function()
local Event = game:GetService("ReplicatedStorage").Ml.SellInventory
Event:FireServer()
end)
Tab:Line()
Tab:Toggle("Auto Mines (Up)", "Automatically mines up for you!", function(v)
_G.AutoMinesUp = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoMinesUp then
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(0, 10, 0),
    0.8
}))
end
end)
end)
Tab:Toggle("Auto Mines (Around)", "Automatically mines around for you!", function(v)
_G.AutoMinesAround = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoMinesAround then
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(-2, 0, -2),
    0.8
}))
end
end)
end)
Tab:Toggle("Auto Mines (Down)", "Automatically mines down for you!", function(v)
_G.AutoMinesDown = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoMinesDown then
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(0, -10, 0),
    0.8
}))
end
end)
end)
Tab:Button("Mine Up", "Mine up button!", function()
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(0, 10, 0),
    0.8
}))
end)
Tab:Button("Mine Around", "Mine around button!", function()
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(-2, 0, -2),
    0.8
}))
end)
Tab:Button("Mine Down", "Mine down button!", function()
local Event = game:GetService("ReplicatedStorage")["shared/network/MiningNetwork@GlobalMiningEvents"].Mine
Event:FireServer(table.unpack({
    Vector3.new(0, -10, 0),
    0.8
}))
end)
